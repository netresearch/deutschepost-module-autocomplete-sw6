<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="PostDirekt\Autocomplete\Service\ModuleConfig">
            <argument id="Shopware\Core\System\SystemConfig\SystemConfigService" type="service"/>
        </service>
        <service id="PostDirekt\Sdk\Autocomplete\Service\ServiceFactory"/>
        <service id="PostDirekt\Autocomplete\Service\AuthenticationService">
            <argument id="PostDirekt\Autocomplete\Service\ModuleConfig" type="service"/>
            <argument id="PostDirekt\Sdk\Autocomplete\Service\ServiceFactory" type="service"/>
            <argument id="monolog.logger" type="service" />
        </service>
    </services>
</container>
